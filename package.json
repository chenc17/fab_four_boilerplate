{
  "name": "electron_react",
  "version": "0.1.0",
  "homepage": "./",
  "main": "electron/main.js",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "python-shell": "^1.0.8",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-start": "rm -r python_build & rm -r python_dist & rm -r python_spec & npm run build && electron .",
    "pyinstaller_to_db": "pyinstaller -F ./python/to_db.py --workpath ./python_build --distpath ./python_dist --specpath ./python_spec",
    "pyinstaller_access_data": "pyinstaller -F ./python/access_data.py --workpath ./python_build --distpath ./python_dist --specpath ./python_spec",
    "package-electron": "npm run build && npm run pyinstaller_to_db && npm run pyinstaller_access_data && electron-builder build -c.extraMetadata.main=electron/main.js --publish never"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^9.4.0",
    "electron-packager": "^13.1.1",
    "electron-rebuild": "^1.8.4",
    "electron-builder": "^22.4.1",
    "typescript": "^3.8.3"
  },
  "build": {
    "appId": "nonExistentID",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "electron/**/*",
      "python_dist/**/*",
      "src/shared/comm.js"
    ]
  }
}
